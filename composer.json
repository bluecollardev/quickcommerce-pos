{
	"name": "firebrand/quickcommerce",
    "type": "project",
    "description": "Firebrand's QuickCommerce Suite for OpenCart featuring full Quickbooks integration and a ReactJS mobile point-of-sale app",
    "keywords": ["quickcommerce", "quickbooks", "opencart", "ecommerce", "framework", "opensource", "integration"],
    "homepage": "http://www.quickcommerce.org",
    "license": "Proprietary for now, while I figure out exactly what my marketing strategy is... Some components will be released for free at a later date",
    "keywords": ["quickcommerce"],
    "authors": [
        {
            "name": "Lucas Lopatka",
            "email": "info@firebrandwebsolutions.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
		"cardinity/cardinity-sdk-php": "^1.0",
        "braintree/braintree_php" : "3.2.0",
        "leafo/scssphp": "0.0.12",
        "divido/divido-php": ">=1.1.1",
        "klarna/kco_rest": "^2.2",
		"symfony/class-loader": "2.7.*",
        "symfony/console": "2.7.*",
        "doctrine/common": "2.5.*",
        "doctrine/dbal": "2.5.*",
        "doctrine/orm": "2.5.*",
		"slim/slim": "^3.1",
        "slim/middleware": "^1.0",
		"slim/twig-view": "^1.1",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "ddeboer/data-import": "0.18.0",
        "mikecao/flight": "@stable",
        "hakre/xmlreaderiterator": "^0.1.10",
        "hakre/xmlutil": "^0.0.1",
        "zendframework/zend-code": "^3.0",
        "firebase/php-jwt": "^4.0",
        "tuupola/slim-jwt-auth": "^2.3"
    },
	"autoload": {
        "psr-4": {
            "App\\": "vendor/quickcommerce/src",
            "QuickCommerce\\API\\": "vendor/quickcommerce/src/API",
            "QuickCommerce\\API\\Service\\": "vendor/quickcommerce/src/API/Service",
            "QuickCommerce\\API\\Exception\\": "vendor/quickcommerce/src/Exception",
            "QuickCommerce\\Adapter\\": "vendor/quickcommerce/src/Adapter/Opencart2x",
            "QuickCommerce\\Adapter\\Oc21\\": "vendor/quickcommerce/src/Adapter/Opencart2x/Oc21",
            "QuickCommerce\\Adapter\\Oc22\\": "vendor/quickcommerce/src/Adapter/Opencart2x/Oc22",
            "QuickCommerce\\Adapter\\Oc23\\": "vendor/quickcommerce/src/Adapter/Opencart2x/Oc23",
            "QuickCommerce\\Adapter\\OC2\\Service\\": "vendor/quickcommerce/src/Adapter/Opencart2x/Service",
            "QuickCommerce\\Model\\": "vendor/quickcommerce/src/Model",
            "QuickCommerce\\Util\\": "vendor/quickcommerce/src/Utils"
        }
    }
}
